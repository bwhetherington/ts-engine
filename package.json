  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack --progress --mode=production",
    "watch:client": "webpack --watch --mode=development",
    "watch:client:silent": "npm run watch:client > /dev/null",
    "build:server": "tsc --project config/server/tsconfig.json",
    "build": "run-s build:client build:server",
    "server": "NODE_ENV=production ts-node -r tsconfig-paths/register --project config/server/tsconfig.json src/server/main.ts",
    "start": "NODE_ENV=production NODE_PATH=build/server/ node build/server/server/main.js",
    "server:dev": "PORT=8000 nodemon",
    "dev": "run-p -s watch:client:silent server:dev",
    "format": "prettier --write ./src"
  },
